{
  "parser": "babel-eslint",
  "extends": "standard",
  "env": {
     "browser": true,
     "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "fetch": true,
    "location": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "operator-linebreak": "off",
    "no-unused-vars": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }]
  }
}
